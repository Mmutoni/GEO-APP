pipeline{
    agent any
    tools{
        maven 'M2_HOME'
    }
    stages{
        stage("analysis"){
            steps{
                withSonarQubeEnv('sonarqube'){
                sh'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=Mmutoni_GEO-APP'
            }
            }
    
        }
        stage("maven1"){
            steps{
            sh 'mvn clean validate compile install test package'
        }
    }
    stage('upload artifact'){
        steps{
            sh 'curl -uadmin:AP8gcgmmset5jeYChTJYDN6XmDd -T \
             target/bio*.jar \
            "http://ec2-54-234-181-248.compute-1.amazonaws.com:8081/artifactory/Geosolution/geo/bio_${BUILD_ID}.jar"'
        }
    }
}
}